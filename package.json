{
  "name": "bosco-alt",
  "version": "6.0.3",
  "description": "Bosco will take care of your microservices, just don't try and use him on a plane.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "nyc mocha",
    "compile-help": "node ./compile-help.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm audit"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 10"
  },
  "keywords": [
    "micro",
    "service",
    "build",
    "automation",
    "project"
  ],
  "bin": {
    "ba": "./bin/ba.js"
  },
  "man": [
    "./man/ba-activity.3",
    "./man/ba-clean.3",
    "./man/ba-clone.3",
    "./man/ba-commit.3",
    "./man/ba-config.3",
    "./man/ba-exec.3",
    "./man/ba-grep.3",
    "./man/ba-install.3",
    "./man/ba-morning.3",
    "./man/ba-ps.3",
    "./man/ba-pull-docker.3",
    "./man/ba-pull-git.3",
    "./man/ba-pull.3",
    "./man/ba-run.3",
    "./man/ba-setup.3",
    "./man/ba-stop.3",
    "./man/ba-team.3",
    "./man/ba-template.3",
    "./man/ba-upstream.3"
  ],
  "directories": {
    "bin": "./bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cliftonc/bosco-alt.git"
  },
  "author": "clifton.cunningham@gmail.com",
  "license": "MIT",
